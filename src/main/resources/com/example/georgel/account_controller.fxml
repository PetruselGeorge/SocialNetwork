            </ImageView>
         </children>
      </AnchorPane>
      <Button fx:id="accountControllerFriendRequest" layoutX="14.0" layoutY="200.0" mnemonicParsing="false" onAction="#accountControllerFriendRequestOnAction" prefHeight="42.0" prefWidth="135.0" style="-fx-background-color: #15002C;" text="Friend Requests" textFill="WHITE">
