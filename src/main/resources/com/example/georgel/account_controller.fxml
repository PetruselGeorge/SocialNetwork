            <Font name="Calibri Light" size="13.0" />
         </font>
      </Button>
      <Button fx:id="accountControllerLogoutButton" layoutX="236.0" layoutY="454.0" mnemonicParsing="false" onAction="#accountControllerLogoutButtonOnAction" prefHeight="34.0" prefWidth="122.0" style="-fx-background-color: #15002C;" text="Logout" textFill="WHITE">
         <font>
            <Font name="Calibri Light" size="13.0" />
         </font>
      </Button>
      <Button fx:id="accountControllerAddFriend" layoutX="14.0" layoutY="140.0" mnemonicParsing="false" onAction="#accountControllerAddFriendOnAction" prefHeight="42.0" prefWidth="135.0" style="-fx-background-color: #15002C;" text="Add Friends" textFill="WHITE">
